<mapper namespace="org.traffic.ldms.request.RequestMapper">
	<insert id="insert"
		parameterType="org.traffic.ldms.request.Request">
		INSERT INTO REQUEST
		VALUES (Request_sequence.NEXTVAL, #{vehicleNo},#{id})
	</insert>
</mapper>